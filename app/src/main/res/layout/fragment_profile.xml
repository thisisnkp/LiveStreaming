                                        android:layout_marginStart="20dp"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/amidum"
                                        android:text="My Posts"
                                        android:textColor="@color/white"
                                        android:textSize="14dp" />

                                    <ImageView
                                        android:id="@+id/imgMyPost"
                                        android:layout_width="35dp"
                                        android:layout_height="35dp"
                                        android:src="@drawable/ic_round_keyboard_arrow_right_24"
                                        app:tint="@color/white" />
                                </LinearLayout>


                                <LinearLayout
                                    android:id="@+id/lytMyVideos"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginVertical="6dp"
                                    android:background="@drawable/stroke_bg_10dp"
                                    android:gravity="center"
                                    android:orientation="horizontal"
                                    android:padding="6dp">

                                    <ImageView
                                        android:layout_width="45dp"
                                        android:layout_height="45dp"
                                        android:background="@drawable/bg_5dp"
                                        android:backgroundTint="#9C27B0"
                                        android:padding="8dp"
                                        android:src="@drawable/videocamara"
                                        app:tint="@color/white" />

                                    <TextView
                                        android:id="@+id/tvMyRelite"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="20dp"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/amidum"
                                        android:text="My Relites"
                                        android:textColor="@color/white"
                                        android:textSize="14dp" />

                                    <ImageView
                                        android:id="@+id/imgMyRelites"
                                        android:layout_width="35dp"
                                        android:layout_height="35dp"
                                        android:src="@drawable/ic_round_keyboard_arrow_right_24"
                                        app:tint="@color/white" />
                                </LinearLayout>

                                <!--                        <View-->
                                <!--                            android:layout_width="match_parent"-->
                                <!--                            android:layout_height=".3dp"-->
