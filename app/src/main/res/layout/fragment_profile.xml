                                        android:src="@drawable/diamond"
                                        app:tint="@color/white" />

                                    <TextView
                                        android:id="@+id/tvFreeDiamond"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="20dp"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/amidum"
                                        android:text="Free Diamond"
                                        android:textColor="@color/white"
                                        android:textSize="14dp" />

                                    <ImageView
                                        android:id="@+id/imgFreeDiamond"
                                        android:layout_width="35dp"
                                        android:layout_height="35dp"
                                        android:src="@drawable/ic_round_keyboard_arrow_right_24"
                                        app:tint="@color/white" />
                                </LinearLayout>


                                <LinearLayout
                                    android:id="@+id/lytVIP"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginVertical="6dp"
                                    android:background="@drawable/stroke_bg_10dp"
                                    android:gravity="center"
                                    android:orientation="horizontal"
                                    android:padding="6dp">

                                    <ImageView
                                        android:layout_width="45dp"
                                        android:layout_height="45dp"
                                        android:background="@drawable/bg_5dp"
                                        android:backgroundTint="#009688"
                                        android:padding="8dp"
                                        android:src="@drawable/crown"
                                        app:tint="@color/white" />

                                    <TextView
                                        android:id="@+id/tvBecomeVip"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="20dp"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/amidum"
                                        android:text="Become VIP"
                                        android:textColor="@color/white"
                                        android:textSize="14dp" />

                                    <ImageView
                                        android:id="@+id/imgBecomeVip"
                                        android:layout_width="35dp"
                                        android:layout_height="35dp"
                                        android:src="@drawable/ic_round_keyboard_arrow_right_24"
                                        app:tint="@color/white" />
                                </LinearLayout>


                                <LinearLayout

                                    android:id="@+id/lytSupport"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginVertical="6dp"
                                    android:background="@drawable/stroke_bg_10dp"
                                    android:gravity="center"
                                    android:orientation="horizontal"
                                    android:padding="6dp">

                                    <ImageView
                                        android:layout_width="45dp"
                                        android:layout_height="45dp"
                                        android:background="@drawable/bg_5dp"
                                        android:backgroundTint="#F44336"
                                        android:padding="8dp"
                                        android:src="@drawable/support"
                                        app:tint="@color/white" />

                                    <TextView
                                        android:id="@+id/tvHaveIssue"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="20dp"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/amidum"
                                        android:text="Have an Issue"
                                        android:textColor="@color/white"
                                        android:textSize="14dp" />

                                    <ImageView
                                        android:id="@+id/imgHaveIssue"
                                        android:layout_width="35dp"
                                        android:layout_height="35dp"
                                        android:src="@drawable/ic_round_keyboard_arrow_right_24"
                                        app:tint="@color/white" />
                                </LinearLayout>


                                <LinearLayout

                                    android:id="@+id/lytComplains"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
